{
  "name": "@sshadows/objid-mcp",
  "version": "2.0.9",
  "description": "MCP server for AL Object ID Ninja - Provides intelligent object ID management for Microsoft Dynamics 365 Business Central development",
  "main": "dist/v2/server.js",
  "bin": {
    "objid-mcp": "./dist/v2/server.js"
  },
  "files": [
    "dist/v2/**/*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.v2.json",
    "build:watch": "tsc -p tsconfig.v2.json --watch",
    "build:v1": "tsc",
    "start": "node dist/v2/server.js",
    "start:v1": "node dist/server.js",
    "dev": "tsx watch src/v2/server.ts",
    "dev:v1": "ts-node src/server.ts",
    "dev:local": "BACKEND_URL=http://localhost:7071 npm run dev",
    "test": "jest --config jest.config.v2.js",
    "test:unit": "jest --config jest.config.v2.js --testPathIgnorePatterns=e2e --testPathIgnorePatterns=realbackend",
    "test:v1": "jest",
    "test:e2e": "jest --config jest.config.v2.js tests/v2/e2e --runInBand --forceExit",
    "test:e2e:watch": "jest --config jest.config.v2.js tests/v2/e2e --runInBand --watch",
    "test:integration": "jest tests/integration --testTimeout=30000",
    "test:local": "BACKEND_URL=http://localhost:7071 npm run test",
    "test:live": "ts-node test-backend-live.ts",
    "lint": "eslint src --ext ts",
    "typecheck": "tsc --noEmit -p tsconfig.v2.json",
    "typecheck:v1": "tsc --noEmit",
    "check": "npm run typecheck && npm run lint",
    "backend:start": "cd ../azure-function-app && npm start",
    "prerelease": "npm run clean && npm run check && npm run test && npm run build",
    "prerelease:quick": "npm run clean && npm run typecheck && npm run build",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mcp",
    "al",
    "business-central",
    "object-id",
    "dynamics-365",
    "model-context-protocol",
    "claude"
  ],
  "author": "SShadowS",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SShadowS/objid-mcp.git"
  },
  "homepage": "https://github.com/SShadowS/objid-mcp#readme",
  "bugs": {
    "url": "https://github.com/SShadowS/objid-mcp/issues"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.18.2",
    "@types/glob": "^9.0.0",
    "axios": "^1.12.2",
    "comment-json": "^4.2.5",
    "dotenv": "^17.2.2",
    "glob": "^11.0.3",
    "zod": "^4.1.11"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.1",
    "eslint": "^9.36.0",
    "jest": "^30.1.3",
    "prettier": "^3.0.0",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.4.4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.7.2"
  }
}
