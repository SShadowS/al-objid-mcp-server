# Test Environment Configuration

# Backend Configuration for Testing
BACKEND_URL=http://localhost:7071
BACKEND_API_VERSION=v2
BACKEND_API_KEY=

# Server Configuration
MCP_MODE=lite
LOG_LEVEL=info
NODE_ENV=test

# Test Specific
TEST_TIMEOUT=10000
TEST_RETRIES=2
TEST_PARALLEL=true

# Feature Flags
ENABLE_CACHING=false
ENABLE_TELEMETRY=false
ENABLE_V1_COMPATIBILITY=true

# Mock Settings
USE_MOCK_BACKEND=false       # Set to true for unit tests
MOCK_RESPONSES=false